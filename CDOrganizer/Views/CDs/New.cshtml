<h1>Add a new CD to @Model.GetName()'s discography</h1>

<form action='/artists/@Model.GetId()/cds' method='post'>
  <input id='artistId' name='artistId' type='hidden' value='@Model.GetId()'>
  <label for='cdTitle'>CD title:</label>
  <input id='cdTitle' name='cdTitle' type='text'>
  <button type='submit'>Add CD</button>
</form>
